<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="40px"
                android:paddingRight="40px">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_above="@+id/cons_bottom_menu"
        android:layout_marginBottom="10px"
        android:background="@drawable/bg_rectangle_login_server"
        android:gravity="center"
        android:text="查询条件"
        android:textColor="#666"/>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/cons_bottom_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/bg_rectangle_login_server"
        android:paddingBottom="20px"
        android:paddingRight="20px"
        >

        <TextView
            android:id="@+id/tv_start_time_tip"
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:gravity="center"
            android:text="@string/start_time_tip"
            android:textColor="#666"
            app:layout_constraintTop_toBottomOf="parent"
            app:layout_editor_absoluteX="50px"
            />

        <TextView
            android:id="@+id/tv_start_time"
            android:layout_width="0dp"
            android:layout_height="60px"
            android:background="@drawable/bg_set_userinfo"
            android:gravity="center_vertical"
            android:paddingLeft="20px"
            android:text="2017-06-14"
            app:layout_constraintBaseline_toBaselineOf="@id/tv_start_time_tip"
            app:layout_constraintLeft_toRightOf="@id/tv_start_time_tip"
            app:layout_constraintRight_toRightOf="parent"/>

        <TextView
            android:id="@+id/tv_end_time_tip"
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:gravity="center"
            android:text="@string/end_time_tip"
            android:textColor="#666"
            app:layout_constraintTop_toBottomOf="@id/tv_start_time_tip"
            app:layout_editor_absoluteX="50px"
            />

        <TextView
            android:id="@+id/tv_end_time"
            android:layout_width="0dp"
            android:layout_height="60px"
            android:background="@drawable/bg_set_userinfo"
            android:gravity="center_vertical"
            android:paddingLeft="20px"
            android:text="2017-06-14"
            app:layout_constraintBaseline_toBaselineOf="@id/tv_end_time_tip"
            app:layout_constraintLeft_toRightOf="@id/tv_end_time_tip"
            app:layout_constraintRight_toRightOf="parent"/>

        <TextView
            android:id="@+id/tv_receive_material_orderno_tip"
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:gravity="center"
            android:text="@string/receive_material_orderno_tip"
            android:textColor="#666"
            app:layout_constraintTop_toBottomOf="@id/tv_end_time_tip"
            app:layout_editor_absoluteX="20px"
            />

        <TextView
            android:id="@+id/tv_receive_material_orderno"
            android:layout_width="0dp"
            android:layout_height="60px"
            android:background="@drawable/bg_set_userinfo"
            android:gravity="center_vertical"
            android:paddingLeft="20px"
            android:text="2017-06-14"
            app:layout_constraintBaseline_toBaselineOf="@id/tv_receive_material_orderno_tip"
            app:layout_constraintLeft_toRightOf="@id/tv_receive_material_orderno_tip"
            app:layout_constraintRight_toRightOf="parent"/>

        <TextView
            android:id="@+id/tv_goods_code_tip"
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:gravity="center"
            android:text="@string/goods_code_tip"
            android:textColor="#666"
            app:layout_constraintTop_toBottomOf="@id/tv_receive_material_orderno_tip"
            app:layout_editor_absoluteX="50px"
            />

        <TextView
            android:id="@+id/tv_goods_code"
            android:layout_width="0dp"
            android:layout_height="60px"
            android:background="@drawable/bg_set_userinfo"
            android:gravity="center_vertical"
            android:paddingLeft="20px"
            android:text="2017-06-14"
            app:layout_constraintBaseline_toBaselineOf="@id/tv_goods_code_tip"
            app:layout_constraintLeft_toRightOf="@id/tv_goods_code_tip"
            app:layout_constraintRight_toRightOf="parent"/>

        <TextView
            android:id="@+id/tv_goods_name_tip"
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:gravity="center"
            android:text="@string/goods_code_tip"
            android:textColor="#666"
            app:layout_constraintTop_toBottomOf="@id/tv_goods_code_tip"
            app:layout_editor_absoluteX="50px"
            />

        <TextView
            android:id="@+id/tv_goods_name"
            android:layout_width="0dp"
            android:layout_height="60px"
            android:background="@drawable/bg_set_userinfo"
            android:gravity="center_vertical"
            android:paddingLeft="20px"
            android:text="2017-06-14"
            app:layout_constraintBaseline_toBaselineOf="@id/tv_goods_name_tip"
            app:layout_constraintLeft_toRightOf="@id/tv_goods_name_tip"
            app:layout_constraintRight_toRightOf="parent"/>

        <TextView
            android:id="@+id/tv_supplier_name_tip"
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:gravity="center"
            android:text="@string/supplier_name_tip"
            android:textColor="#666"
            app:layout_constraintTop_toBottomOf="@id/tv_goods_name_tip"
            app:layout_editor_absoluteX="20px"
            />

        <TextView
            android:id="@+id/tv_supplier_name"
            android:layout_width="0dp"
            android:layout_height="60px"
            android:background="@drawable/bg_set_userinfo"
            android:gravity="center_vertical"
            android:paddingLeft="20px"
            android:text="2017-06-14"
            app:layout_constraintBaseline_toBaselineOf="@id/tv_supplier_name_tip"
            app:layout_constraintLeft_toRightOf="@id/tv_supplier_name_tip"
            app:layout_constraintRight_toRightOf="parent"/>

        <TextView
            android:id="@+id/tv_pack_name_tip"
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:gravity="center"
            android:text="@string/tv_pack_name_tip"
            android:textColor="#666"
            app:layout_constraintTop_toBottomOf="@id/tv_supplier_name_tip"
            app:layout_editor_absoluteX="50px"
            />

        <TextView
            android:id="@+id/tv_pack_name"
            android:layout_width="0dp"
            android:layout_height="60px"
            android:background="@drawable/bg_set_userinfo"
            android:gravity="center_vertical"
            android:paddingLeft="20px"
            android:text="2017-06-14"
            app:layout_constraintBaseline_toBaselineOf="@id/tv_pack_name_tip"
            app:layout_constraintLeft_toRightOf="@id/tv_pack_name_tip"
            app:layout_constraintRight_toRightOf="parent"/>

        <View
            android:id="@+id/view_divide"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_marginTop="20px"
            android:background="#999"
            app:layout_constraintTop_toBottomOf="@id/tv_pack_name"/>

        <TextView
            android:layout_width="200px"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20px"
            android:layout_marginLeft="30px"
            android:layout_marginTop="20px"
            android:background="@drawable/bg_sbo_rectangle_et"
            android:gravity="center"
            android:padding="20px"
            android:text="@string/cancel"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view_divide"/>

        <TextView
            android:layout_width="200px"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20px"
            android:layout_marginRight="30px"
            android:layout_marginTop="20px"
            android:background="@drawable/bg_sbo_rectangle_et"
            android:gravity="center"
            android:padding="20px"
            android:text="@string/confirm"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view_divide"/>
    </android.support.constraint.ConstraintLayout>
</RelativeLayout>
