<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                     android:layout_width="match_parent"
                                     android:layout_height="wrap_content"
                                     android:background="#fff"
                                     android:orientation="vertical">

    <TextView
        android:id="@+id/tv_head_title"
        android:layout_width="match_parent"
        android:layout_height="72px"
        android:gravity="center_vertical"
        android:paddingLeft="20px"
        android:paddingTop="10px"
        android:text="委外发料单信息"
        android:textColor="#333"
        android:textSize="28px"
        android:textStyle="bold"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#d7d7dd"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_orderno_tip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20px"
            android:text="@string/outsource_orderno_tip"
            android:textColor="#333"
            android:textSize="26px"/>

        <TextView
            android:id="@+id/tv_outsource_orderno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="60px"
            android:text=""
            android:textColor="#000"
            android:textSize="26px"/>
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_create_orderno_date_tip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20px"
            android:text="@string/create_orderno_date_tip"
            android:textColor="#333"
            android:textSize="26px"/>

        <TextView
            android:id="@+id/tv_create_orderno_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="35px"
            android:text=""
            android:textColor="#000"
            android:textSize="26px"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_stock_name_tip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20px"
            android:text="@string/stock_name_tip"
            android:textColor="#333"
            android:textSize="26px"/>

        <TextView
            android:id="@+id/tv_stock_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="60px"
            android:text=""
            android:textColor="#000"
            android:textSize="26px"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_strict_name_tip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20px"
            android:text="@string/strict_name_tip"
            android:textColor="#333"
            android:textSize="26px"/>

        <TextView
            android:id="@+id/tv_strict_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="60px"
            android:text=""
            android:textColor="#000"
            android:textSize="26px"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_send_material_total_num_tip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20px"
            android:text="@string/send_material_total_num_tip"
            android:textColor="#333"
            android:textSize="26px"/>

        <TextView
            android:id="@+id/tv_buy_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="60px"
            android:text=""
            android:textColor="#000"
            android:textSize="26px"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20px"
            android:text="@string/have_get_total_num_tip"
            android:textColor="#333"
            android:textSize="26px"/>

        <TextView
            android:id="@+id/tv_have_get_total_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="60px"
            android:text=""
            android:textColor="#000"
            android:textSize="26px"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_can_get_total_num_tip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20px"
            android:text="@string/can_get_total_num_tip"
            android:textColor="#333"
            android:textSize="26px"/>

        <TextView
            android:id="@+id/tv_can_get_total_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="60px"
            android:text=""
            android:textColor="#000"
            android:textSize="26px"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_wait_count_num_tip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20px"
            android:text="@string/wait_point_num_tip"
            android:textColor="#333"
            android:textSize="26px"/>

        <TextView
            android:id="@+id/tv_wait_point_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="60px"
            android:text=""
            android:textColor="#000"
            android:textSize="26px"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20px"
        android:layout_marginTop="20px"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_have_count_num_tip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="20px"
            android:text="@string/have_point_num_tip"
            android:textColor="#333"
            android:textSize="26px"/>

        <TextView
            android:id="@+id/tv_have_count_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="60px"
            android:text=""
            android:textColor="#000"
            android:textSize="26px"/>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:layout_marginLeft="30px"
        android:background="@color/tab_txt_unselect"/>
</com.zhy.autolayout.AutoLinearLayout>
