<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                       android:layout_width="match_parent"

                                       android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@drawable/bg_rectangle_login_server"
        android:orientation="vertical"
        android:padding="20px">
        <TextView
            android:id="@+id/tv_from_orderno"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:gravity="center_vertical"
            android:text="@string/dialog_from_orderno"
            android:textColor="#333"
            android:textSize="14sp"/>
        <TextView
            android:id="@+id/tv_supplier"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:gravity="center_vertical"
            android:paddingLeft="10px"
            android:text="@string/buy_from"
            android:textColor="#333"
            android:textSize="14sp"/>
        <TextView
            android:id="@+id/tv_create_orderno_date"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:gravity="center_vertical"
            android:text="@string/create_orderno_date"
            android:textColor="#333"
            android:textSize="14sp"/>
        <TextView
            android:id="@+id/tv_material_code"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:gravity="center_vertical"
            android:text="@string/material_code"
            android:textColor="#333"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/tv_material_name"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:gravity="center_vertical"
            android:text="@string/material_name"
            android:textColor="#333"
            android:textSize="14sp"/>
        >
        <TextView
            android:id="@+id/tv_material_attr"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:gravity="center_vertical"
            android:text="@string/material_attr"
            android:textColor="#333"
            android:textSize="14sp"/>
        >

        <TextView
            android:id="@+id/tv_material_model"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:gravity="center_vertical"
            android:text="@string/material_model"
            android:textColor="#333"
            android:textSize="14sp"/>
        <TextView
            android:id="@+id/tv_pack_num"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:gravity="center_vertical"
            android:text="@string/pack_num"
            android:textColor="#333"
            android:paddingLeft="30px"
            android:textSize="14sp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:orientation="horizontal"
            >

            <TextView
                android:id="@+id/tv_stockin_point_sparenum"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="@string/dialog_reject_num"
                android:paddingLeft="30px"
                android:textColor="#333"
                android:textSize="14sp"/>

            <EditText
                android:id="@+id/et_reject_num"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10px"
                android:layout_marginTop="10px"
                android:background="@drawable/bg_sbo_rectangle_et"
                android:gravity="center_vertical"
                android:hint="@string/please_input_reject_num"
                android:inputType="number"
                android:maxLines="1"
                android:paddingLeft="20px"
                android:textSize="14sp"/>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:layout_marginTop="40px">

            <Button
                android:id="@+id/btn_cancel"
                android:layout_width="wrap_content"
                android:layout_height="80px"
                android:layout_marginLeft="100px"
                android:background="@drawable/bg_sbo_rectangle_et"
                android:text="@string/cancel"
                android:textColor="#666"/>

            <Button
                android:id="@+id/btn_commit"
                android:layout_width="wrap_content"
                android:layout_height="80px"
                android:layout_alignParentRight="true"
                android:layout_marginRight="100px"
                android:background="@drawable/bg_sbo_rectangle_et"
                android:text="@string/confirm"
                android:textColor="#666"/>
        </RelativeLayout>
    </LinearLayout>
</com.zhy.autolayout.AutoRelativeLayout>
